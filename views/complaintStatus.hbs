<div class="container">
    <div class="text-center heading-margin">
        <p>&nbsp;</p>
    </div>

        <div class="categoryContainer">
       <h4 class="">Complaint Status</h4>
        <p>View Status of existing complaint</p>
        <table style="width:100%">
            <tr>
                <th>ComplaintID</th>
                <th>Header</th>
                <th>Complaint status</th>
            </tr>
            {{#each usrComplt}}
            <tr>
                <th>{{usrComplt.complaintID}}</th>
                <th>{{usrComplt.uccHeader}}</th>
                <th>{{usrComplt.uccStatus}}</th>
            </tr>
            {{/each}}
        </table>
    </div>
</div>