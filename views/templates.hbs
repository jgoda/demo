<div class="container">
    <div class="text-center heading-margin">
        <p>&nbsp;</p>
    </div>

    <div class="categoryContainer">
        <ul class="collection">
            <li class="collection-header noBorder"><h4>Templates</h4></li>
            <div class="greyBorder">
                {{#each templates}}
                    <li class="collection-item avatar">
                        <span class="title">{{this.contentTemplateID}}</span>
                        <p>{{this.contentType}} <br>
                            {{this.contentCategory}}
                        </p>
                    </li>

                {{/each}}
            </div>
        </ul>
    </div>


</div>

