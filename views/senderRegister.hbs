<div class="container">
    <div class="text-center heading-margin">
        <p>Sender</p>
    </div>

    <div class="categoryContainer">
        <div class="row">
            <div class="input-field col s12">
                <input id="sender_name" type="text" class="validate">
                <label for="icon_prefix">Name</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="sender_companyType" type="text" class="validate">
                <label for="icon_prefix">Company Type</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="sender_gst" type="text" class="validate">
                <label for="icon_prefix">GST Number</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="sender_pan" type="text" class="validate">
                <label for="icon_prefix">PAN</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="sender_kyc" type="file" class="validate">
            </div>
        </div>
        <a class="waves-effect waves-light btn" onclick="registerSender()">Register</a>

    </div>


</div>

